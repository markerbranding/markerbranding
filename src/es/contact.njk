      <form id="custom-hubspot-form" name="contacto" method="POST">
        <div>
          <label for="name">Nombre y Apellido: <span>*</span></label>
          <input type="text" id="name" name="name" required pattern="[A-Za-z\s]+" maxlength="50" title="Solo letras y espacios permitidos" />
        </div>
        <div>
          <label for="telefono">Teléfono: <span>*</span></label>
          <input type="text" id="telefono" name="telefono" required pattern="\d{10}" maxlength="10" title="Debe ser un número de 10 dígitos" />
        </div>
        <div>
          <label for="email">Correo electrónico: <span>*</span></label>
          <input type="email" id="email" name="email" required maxlength="100" />
        </div>
        <div>
          <label for="empresa">Empresa:</label>
          <input type="text" id="empresa" name="empresa" maxlength="100" />
        </div>
        <div>
          <label for="ciudad">Ciudad: <span>*</span></label>
          <input type="text" id="ciudad" name="ciudad" required maxlength="100" />
        </div>
        <div>
          <label for="role">Elige un servicio de interés: <span>*</span></label>
          <div class="select__wrapper">
            <select id="role" name="role[]" required>
              <option disabled selected value>-- Selecciona una opción --</option>
              <option value="Branding">Branding (Estrategia + Identidad)</option>
              <option value="Web">Desarrollo de Sitio Web</option>
              <option value="Marketing">Marketing (Estrategia + Gestión mensual)</option>
              <option value="Otro">Otro</option>
            </select>
          </div>
        </div>
        <div>
          <label for="message">Comentarios:</label>
          <textarea id="message" name="message" rows="4" maxlength="500"></textarea>
        </div>
        
        <div>
          <div style="display: none;">
            <input type="text" name="honeypot" tabindex="-1" autocomplete="off" />
          </div>
          <div class="g-recaptcha" data-sitekey="6LdGCa8qAAAAAOt3UR8sQreBUL_utZO0CQLFKC5A"></div>
          <script src="https://www.google.com/recaptcha/api.js" async defer></script>
          <button class="btn" type="submit">Enviar mensaje</button>
        </div>
      </form>

          
          
  
          
